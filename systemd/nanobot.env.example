# Paths
NANOBOT_REPO=/opt/nanoBot_memU
NANOBOT_VENV=/opt/nanobot-venv

# LLM (MiniMax)
MINIMAX_API_KEY=REPLACE_ME
MINIMAX_API_BASE=https://api.minimax.io/v1

# MemU LLM (DeepSeek)
DEEPSEEK_API_KEY=REPLACE_ME
DEEPSEEK_BASE_URL=https://api.deepseek.com/v1
DEEPSEEK_CHAT_MODEL=deepseek-chat

# MemU persistence
MEMU_DB_DSN=sqlite:////var/lib/nanobot/memu.db

# WhatsApp Bridge options (optional)
WA_MARK_ONLINE=0
WA_AUTO_READ=0
